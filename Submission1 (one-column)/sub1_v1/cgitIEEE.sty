\makeatletter
\newcommand{\generateb}[4]{% This command puts the new comandletter beofre the prefix e.g: for descriptor = vec range = A-Z will yield \Avec, \Bvec, ... \Zvec
  %#1 = prefix, #2 = macro, #3 = starting point, #4 = end point
  \def\@tempa{#1} % we don't want to lowercase it
  \count@=`#3
  \loop
  \begingroup\lccode`?=\count@
  \lowercase{\endgroup\@namedef{?\@tempa}{#2{?}}}%
  \ifnum\count@<`#4
  \advance\count@\@ne
  \repeat
}

\newcommand{\generatel}[4]{% This command puts the new comandletter beofre the prefix e.g: for descriptor = vec range = A-Z will yield \vecA, \vecB, ... \vecZ
  %#1 = prefix, #2 = macro, #3 = starting point, #4 = end point
  \def\@tempa{#1} % we don't want to lowercase it
  \count@=`#3
  \loop
  \begingroup\lccode`?=\count@
  \lowercase{\endgroup\@namedef{\@tempa ?}{#2{?}}}%
  \ifnum\count@<`#4
  \advance\count@\@ne
  \repeat
}
\makeatother

